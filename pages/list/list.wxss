/* pages/list/list.wxss */
.list-title{
  width: 100%;
  height: 100rpx;
  background: #f4f3ef;
  text-align: center;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #4f4f4f;
}
.list{
  width: 100%;
  height: auto;
  display: flex;
  flex-wrap: wrap;
}
.list .list-wrap{
  width: 25%;
  text-align: center;
  height: 240rpx;
}
.list .list-wrap view:first-child image{
  width: 156rpx;
  height: 156rpx;
}
.list .list-wrap view:last-child{
  width: 100%;
  text-align: center;
  font-size: 20rpx;
}